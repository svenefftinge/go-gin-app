ports:
  - port: 8080
    protocol: "http"
checkoutLocation: "src/github.com/demo-apps/go-gin-app"
workspaceLocation: "/workspace/src/github.com/demo-apps/go-gin-app"
tasks:
  - command: >
      cd /workspace/src/github.com/demo-apps/go-gin-app &&
      go get -v ./... &&
      go build -o app &&
      ./app 